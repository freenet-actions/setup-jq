{
  "name": "typescript-action",
  "version": "3.1.1",
  "private": true,
  "description": "TypeScript template action",
  "main": "lib/main.js",
  "scripts": {
    "build": "tsc",
    "format": "prettier --write **/*.ts",
    "format-check": "prettier --check **/*.ts",
    "lint": "eslint src/**/*.ts",
    "package": "ncc build",
    "test": "jest",
    "all": "npm run build && npm run format && npm run package && npm test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/freenet-actions/setup-jq.git"
  },
  "keywords": [
    "actions",
    "node",
    "setup"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "ansi-regex": ">=6.0.1",
    "set-value": ">=4.0.1",
    "@actions/core": "1.10.1",
    "@actions/tool-cache": "2.0.1"
  },
  "devDependencies": {
    "@types/jest": "28.1.8",
    "@types/node": "20.8.3",
    "@typescript-eslint/parser": "6.7.4",
    "@vercel/ncc": "0.38.0",
    "eslint": "8.51.0",
    "eslint-plugin-github": "4.10.1",
    "eslint-plugin-jest": "27.4.2",
    "jest": "28.1.3",
    "jest-circus": "29.7.0",
    "js-yaml": "4.1.0",
    "prettier": "3.0.3",
    "ts-jest": "28.0.8",
    "typescript": "5.2.2"
  }
}
