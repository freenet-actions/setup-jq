{
  "name": "typescript-action",
  "version": "1.0.3",
  "private": true,
  "description": "TypeScript template action",
  "main": "lib/main.js",
  "scripts": {
    "build": "tsc",
    "format": "prettier --write **/*.ts",
    "format-check": "prettier --check **/*.ts",
    "lint": "eslint src/**/*.ts",
    "package": "ncc build",
    "test": "jest",
    "all": "npm run build && npm run format && npm run package && npm test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/md-actions/setup-jq.git"
  },
  "keywords": [
    "actions",
    "node",
    "setup"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "ansi-regex": ">=6.0.1",
    "set-value": ">=4.0.1",
    "@actions/core": "1.6.0",
    "@actions/tool-cache": "1.7.1"
  },
  "devDependencies": {
    "@types/jest": "27.4.1",
    "@types/node": "17.0.21",
    "@typescript-eslint/parser": "4.33.0",
    "@vercel/ncc": "0.33.3",
    "eslint": "7.32.0",
    "eslint-plugin-github": "4.3.2",
    "eslint-plugin-jest": "26.1.1",
    "jest": "26.6.3",
    "jest-circus": "27.5.1",
    "js-yaml": "4.1.0",
    "prettier": "2.6.0",
    "ts-jest": "26.5.6",
    "typescript": "4.6.2"
  }
}
